{
  "name": "graphest",
  "version": "1.3.3",
  "description": "A faithful graphing calculator",
  "main": "dist/main.js",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:app": "webpack",
    "build:rust": "bash -c \"cargo build --release && mkdir -p dist && rm -f dist/{compose,concatenate,graph} && cp target/release/{compose,concatenate,graph} dist/\"",
    "clean": "npm-run-all clean:*",
    "clean:app": "rm -rf dist release",
    "clean:rust": "rm -rf target",
    "guide": "cd docs/guide && asciidoctor --trace -r ./config/remove_mathjax.rb index.adoc && open index.html",
    "lint": "prettier --check .",
    "release:mac": "npm run lint && npm run clean && NODE_ENV=production npm run build && electron-builder --mac --publish always",
    "release:win": "npm run lint && npm run clean && cross-env-shell NODE_ENV=production npm run build && electron-builder --win --publish always",
    "start": "cross-env-shell NODE_ENV=development npm run build && electron .",
    "test": "jest"
  },
  "author": "Masashi Mizuno",
  "license": "MIT",
  "repository": "github:unageek/graphest",
  "private": true,
  "devDependencies": {
    "@electron/notarize": "^3.1.1",
    "@eslint/js": "^9.39.1",
    "@types/color": "^3.0.3",
    "@types/css-font-loading-module": "0.0.8",
    "@types/eslint": "^9.6.1",
    "@types/jest": "^30.0.0",
    "@types/leaflet": "^1.9.3",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.1",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^18.3.1",
    "@types/resize-observer-browser": "^0.1.11",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "electron": "^39.2.3",
    "electron-builder": "^26.0.12",
    "electron-devtools-installer": "^3.2.0",
    "electron-updater": "^6.6.2",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^4.0.1",
    "globals": "^16.5.0",
    "html-webpack-plugin": "^5.5.1",
    "jest": "^30.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.72.7",
    "@fluentui/react-icons": "^2.0.315",
    "@fontsource/dejavu-mono": "^5.2.5",
    "@fontsource/noto-sans": "^5.2.10",
    "@reduxjs/toolkit": "^2.11.0",
    "bignumber.js": "^9.1.1",
    "color": "^4.2.3",
    "leaflet": "^1.9.4",
    "leaflet-easybutton": "^2.4.0",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "slate": "^0.94.1",
    "slate-history": "^0.93.0",
    "slate-react": "^0.95.0",
    "zod": "^3.22.3"
  },
  "productName": "Graphest"
}
