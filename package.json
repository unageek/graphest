{
  "name": "graphest",
  "version": "1.2.1",
  "description": "A faithful graphing calculator",
  "main": "./dist/main.js",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:app": "webpack",
    "build:rust": "cargo build --release && mkdirp dist && rm -f dist/{compose,concatenate,graph} && cp target/release/{compose,concatenate,graph} dist/",
    "clean": "npm-run-all clean:*",
    "clean:app": "rm -rf dist release",
    "clean:rust": "rm -rf target",
    "guide": "cd docs/guide && asciidoctor --trace -r ./config/remove_mathjax.rb index.adoc && open index.html",
    "lint": "prettier --check .",
    "release:mac": "npm run lint && npm run clean && NODE_ENV=production npm run build && electron-builder --mac --publish always",
    "release:win": "npm run lint && npm run clean && cross-env-shell NODE_ENV=production npm run build && electron-builder --win --publish always",
    "start": "cross-env-shell NODE_ENV=development npm run build && electron .",
    "test": "jest"
  },
  "author": "Masashi Mizuno",
  "license": "MIT",
  "repository": "github:unageek/graphest",
  "private": true,
  "devDependencies": {
    "@types/color": "^3.0.3",
    "@types/css-font-loading-module": "0.0.7",
    "@types/jest": "^27.5.0",
    "@types/leaflet": "^1.7.10",
    "@types/node": "^17.0.35",
    "@types/react": "^17.0.45",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^17.0.16",
    "@types/resize-observer-browser": "^0.1.7",
    "@types/yup": "^0.29.14",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.25.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "electron": "^18.2.3",
    "electron-builder": "^23.0.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-notarize": "^1.2.1",
    "electron-updater": "^5.0.1",
    "eslint": "^8.16.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@fluentui/react": "^8.68.0",
    "@fontsource/dejavu-mono": "^4.5.4",
    "@fontsource/noto-sans": "^4.5.10",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@reduxjs/toolkit": "^1.8.1",
    "bignumber.js": "^9.0.2",
    "color": "^4.2.3",
    "leaflet": "^1.8.0",
    "leaflet-easybutton": "^2.4.0",
    "lodash": "^4.17.21",
    "react": "^17.0.1",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.1",
    "slate": "^0.78.0",
    "slate-history": "^0.66.0",
    "slate-react": "^0.79.0",
    "yup": "^0.32.11"
  },
  "productName": "Graphest"
}
