{
  "name": "graphest",
  "version": "0.5.3",
  "description": "A faithful graphing calculator",
  "main": "./dist/main.js",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:app": "webpack",
    "build:rust": "cargo build --release && mkdir -p dist && rm -f dist/graph && cp target/release/graph dist/",
    "clean": "npm-run-all clean:*",
    "clean:app": "rm -rf dist release",
    "clean:rust": "rm -rf target",
    "lint": "prettier --check .",
    "release:mac": "npm run lint && npm run clean && NODE_ENV=production npm run build && electron-builder --mac --publish always",
    "start": "NODE_ENV=development npm run build && electron ."
  },
  "author": "Masashi Mizuno",
  "license": "MIT",
  "repository": "github:unageek/graphest",
  "private": true,
  "devDependencies": {
    "@types/color": "^3.0.2",
    "@types/css-font-loading-module": "0.0.6",
    "@types/leaflet": "^1.7.5",
    "@types/react": "^17.0.20",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "@types/resize-observer-browser": "^0.1.6",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "css-loader": "^6.2.0",
    "electron": "^14.0.0",
    "electron-builder": "~22.10.5",
    "electron-notarize": "^1.1.1",
    "electron-updater": "^4.3.9",
    "eslint": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^5.3.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.2",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "@fluentui/react": "^8.30.3",
    "@fontsource/noto-sans": "^4.5.0",
    "@reduxjs/toolkit": "^1.6.1",
    "bignumber.js": "^9.0.1",
    "color": "^4.0.1",
    "leaflet": "^1.7.1",
    "leaflet-easybutton": "^2.4.0",
    "react": "^17.0.1",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5"
  },
  "productName": "Graphest"
}
