{
  "name": "graphest",
  "version": "0.8.2",
  "description": "A faithful graphing calculator",
  "main": "./dist/main.js",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:app": "webpack",
    "build:rust": "cargo build --release && mkdirp dist && rm -f dist/graph && cp target/release/graph dist/",
    "clean": "npm-run-all clean:*",
    "clean:app": "rm -rf dist release",
    "clean:rust": "rm -rf target",
    "guide": "cd docs/guide && asciidoctor --trace -r ./config/remove_mathjax.rb index.adoc && open index.html",
    "lint": "prettier --check .",
    "release:mac": "npm run lint && npm run clean && NODE_ENV=production npm run build && electron-builder --mac --publish always",
    "release:win": "npm run lint && npm run clean && cross-env-shell NODE_ENV=production npm run build && electron-builder --win --publish always",
    "start": "cross-env-shell NODE_ENV=development npm run build && electron ."
  },
  "author": "Masashi Mizuno",
  "license": "MIT",
  "repository": "github:unageek/graphest",
  "private": true,
  "devDependencies": {
    "@types/color": "^3.0.2",
    "@types/css-font-loading-module": "0.0.7",
    "@types/leaflet": "^1.7.5",
    "@types/react": "^17.0.34",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.20",
    "@types/resize-observer-browser": "^0.1.6",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "electron": "^15.3.0",
    "electron-builder": "~22.13.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-notarize": "^1.1.1",
    "electron-updater": "^4.3.9",
    "eslint": "^8.2.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^5.5.0",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.64.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@fluentui/react": "^8.41.4",
    "@fontsource/noto-sans": "^4.5.1",
    "@reduxjs/toolkit": "^1.6.2",
    "bignumber.js": "^9.0.1",
    "color": "^4.0.1",
    "leaflet": "^1.7.1",
    "leaflet-easybutton": "^2.4.0",
    "react": "^17.0.1",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6"
  },
  "productName": "Graphest"
}
