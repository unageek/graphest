{
  "name": "graphest",
  "version": "0.4.1",
  "description": "A faithful graphing calculator",
  "main": "./dist/main.js",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:app": "webpack",
    "build:rust": "cd rust && cargo build --release --all-features && mkdir -p ../dist && rm -f ../dist/graph && cp target/release/graph ../dist/",
    "clean": "rm -rf dist && rm -rf release",
    "lint": "prettier --check .",
    "release:mac": "npm run lint && npm run clean && NODE_ENV=production npm run build && electron-builder --mac --publish always",
    "start": "NODE_ENV=development npm run build && electron ."
  },
  "author": "Masashi Mizuno",
  "license": "MIT",
  "repository": "github:unageek/graphest",
  "private": true,
  "devDependencies": {
    "@types/color": "^3.0.2",
    "@types/css-font-loading-module": "0.0.6",
    "@types/leaflet": "^1.7.4",
    "@types/react": "^17.0.14",
    "@types/react-beautiful-dnd": "^13.1.0",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "@types/resize-observer-browser": "^0.1.5",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "css-loader": "^5.2.6",
    "electron": "^13.1.6",
    "electron-builder": "^22.11.7",
    "electron-notarize": "^1.0.0",
    "electron-updater": "^4.3.9",
    "eslint": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.4",
    "html-webpack-plugin": "^5.3.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "@fluentui/react": "^8.22.3",
    "@fontsource/noto-sans": "^4.4.2",
    "@reduxjs/toolkit": "^1.6.0",
    "bignumber.js": "^9.0.1",
    "color": "^3.1.3",
    "leaflet": "^1.7.1",
    "leaflet-easybutton": "^2.4.0",
    "react": "^17.0.1",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4"
  },
  "productName": "Graphest"
}
